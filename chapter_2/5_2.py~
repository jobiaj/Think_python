def first_line():
    print "+","-","-","-","-","+","-","-","-","-","+","+","-","-","-","-","+","-","-","-","-","+"
def second_line():
    print "/", " " * 7, "/" , " " * 7 ,"/", " " * 7, "/" , " " * 7 ,"/" 
first_line() 
second_line()
second_line() 
second_line()  
second_line() 
first_line()  
second_line() 
second_line()
second_line() 
second_line() 
first_line()  
